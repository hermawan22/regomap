{
  "name": "regomap",
  "version": "1.0.0",
  "description": "Wrapper Google Maps for React",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --p",
    "dev": "webpack-dev-server --open",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"app/**/*.{js, jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet || exit 0",
    "start": "NODE_ENV=server nodemon server.js",
    "test": "NODE_ENV=test jest",
    "test:coverage": "jest --coverage",
    "test:update": "NODE_ENV=test jest -u",
    "watch": "webpack --watch"
  },
  "keywords": [
    "Google",
    "Maps",
    "React",
    "Google Maps",
    "Map",
    "React Google Maps",
    "React Google Map",
    "Google Map React",
    "Google Maps React"
  ],
  "author": "hermawan22 <heramawan.remote@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "jest-serializer-enzyme": "^1.0.0",
    "prettier-eslint": "^6.4.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
